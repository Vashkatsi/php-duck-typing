{
  "name": "vashkatsi/ducktype",
  "description": "A PHP library for asserting duck typing.",
  "keywords": [
    "duck typing",
    "type checking",
    "php"
  ],
  "homepage": "https://github.com/vashkatsi/ducktype",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Archil Abuladze",
      "email": "armiworker@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4"
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "DuckType\\": "src/"
    },
    "files": [
      "src/DuckType.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "DuckType\\Tests\\": "tests/"
    }
  }
}
